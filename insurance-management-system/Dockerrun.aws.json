{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "car-insurance-api",
      "image": "alexandrugabrielgrecu/car-insurance-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://localhost:5432/cars"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "endava"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "password"
        }
      ]
    }
  ]
}